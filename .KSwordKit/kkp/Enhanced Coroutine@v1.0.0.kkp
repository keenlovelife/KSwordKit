using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.UI;

public class ExamplesEnhancedCoroutineTest : MonoBehaviour
{
    [Header("UIÂÖÉÁ¥†")]
    public Text Text;
    public Image ProgressImage;
    public Text ProgressText;

    void Start()
    {
        // Â∏∏ËßÅÁöÑÂ¶Ç‰∏ã‰ΩøÁî® for„ÄÅwhile Âíå do...while:
        for (var for_i = 0; for_i < 3; for_i++)
        {
            Debug.Log("ËøôÊòØ for Á¨¨" + for_i + "Ê¨°Âæ™ÁéØ„ÄÇ");
        }

        var while_i = 0;
        while(while_i < 3)
        {
            Debug.Log("ËøôÊòØ while Á¨¨" + while_i + "Ê¨°Âæ™ÁéØ„ÄÇ");
            while_i++;
        }

        var do__while_i = 0;
        do
        {
            Debug.Log("ËøôÊòØ do...while Á¨¨" + do__while_i + "Ê¨°Âæ™ÁéØ„ÄÇ");
            do__while_i++;

        } while (do__while_i < 3);


        // ÊØèÂ∏ßwhileÂæ™ÁéØ‰∏ÄÊ¨°
        var i = 0;
        KSwordKit.Core.EnhancedCoroutine.Loop.While(()=> i < 3, ()=>{
            Debug.Log("ËøôÊòØÂçèÁ®ãwhile Á¨¨"+ i +"Ê¨°Âæ™ÁéØ(ÊØèÂ∏ß)„ÄÇ");
            i++;
        });

        // ÊØè‰∏ÄÁßíÊâßË°å‰∏ÄÊ¨°whileÂæ™ÁéØ
        var j = 0;
        KSwordKit.Core.EnhancedCoroutine.Loop.While(()=> j < 3, ()=>{
            Debug.Log("ËøôÊòØ ÂçèÁ®ãwhile Á¨¨" + j + "Ê¨°While Âæ™ÁéØ(ÊØèÁßí)„ÄÇ");
            j++;
        }, 1);

        // ÊØèÂ∏ßÊâßË°å‰∏ÄÊ¨° do while Âæ™ÁéØ
        var k = 0;
        KSwordKit.Core.EnhancedCoroutine.Loop.Do___While(()=>{
            Debug.Log("ËøôÊòØ ÂçèÁ®ãdo while Á¨¨" + k +"Ê¨°Âæ™ÁéØ(ÊØèÂ∏ß)„ÄÇ");
            k++;
        }, ()=> k < 3);

        // ÊØèÁßíÊâßË°å‰∏ÄÊ¨° do while Âæ™ÁéØ
        var t = 0;
        KSwordKit.Core.EnhancedCoroutine.Loop.Do___While(()=>{
            Debug.Log("ËøôÊòØ ÂçèÁ®ãdo while Á¨¨" + t + "Ê¨°Âæ™ÁéØ(ÊØèÁßí)„ÄÇ");
            t++;
        }, ()=> t < 3, 1);

        // ÊØèÂ∏ßÊâßË°å‰∏ÄÊ¨° for Âæ™ÁéØ
        var h = 0;
        KSwordKit.Core.EnhancedCoroutine.Loop.For(() => h = 0, ()=> h < 3, () => {
            Debug.Log("ËøôÊòØ ÂçèÁ®ãfor Á¨¨" + h + "Ê¨°Âæ™ÁéØ(ÊØèÂ∏ß)„ÄÇ");
        }, ()=> h++);

        // ÊØèÁßíÊâßË°å‰∏ÄÊ¨° for Âæ™ÁéØ
        var p = 0;
        KSwordKit.Core.EnhancedCoroutine.Loop.For(() => p = 0, () => p < 3, () => {
            Debug.Log("ËøôÊòØ ÂçèÁ®ãfor Á¨¨" + p + "Ê¨°Âæ™ÁéØ(ÊØèÁßí)„ÄÇ");
        }, () => p++, 1);

        // Frame Á±ªÁöÑÂ∫îÁî®Âú∫ÊôØ
        bool isdone = false;
        string text = null;
        string error = null;
        var thread = new System.Threading.Thread(() => {
            Debug.Log("Âú®Â≠êÁ∫øÁ®ãËØªÂèñÊñáÊú¨ÔºåThreadId = " + System.Threading.Thread.CurrentThread.ManagedThreadId);
            try
            {
                text = System.IO.File.ReadAllText("Assets/Examples/ExamplesLoop/ExamplesEnhancedCoroutineTest.cs");
                isdone = true;
            }catch(System.Exception e)
            {
                error = e.Message;
                isdone = true;
            }
        });
        thread.Start();
        KSwordKit.Core.EnhancedCoroutine.Frame.WaitWhile(() => !isdone, () => {
            Debug.Log("Âú®UIÁ∫øÁ®ã‰∏≠Êü•ÁúãËØªÂèñÁªìÊûúÔºåThreadId = " + System.Threading.Thread.CurrentThread.ManagedThreadId);

            if (string.IsNullOrEmpty(error))
                Text.text = text;
            else
                Text.text = error;
        });

        float progress = 0;
        float max = 10;
        KSwordKit.Core.EnhancedCoroutine.Loop.For(() => progress = 0, () => progress < 11, ()=> {
            var maxwidth = ProgressImage.transform.parent.GetComponent<RectTransform>().rect.width;
            ProgressImage.rectTransform.sizeDelta = new Vector2(progress / max * maxwidth, ProgressImage.rectTransform.rect.height);
            ProgressText.text = ((progress / max) * 100).ToString("f2") + "%";
            if (progress == 10)
            {
                KSwordKit.Core.EnhancedCoroutine.Frame.WaitTime(1, () => {
                    ProgressText.text = "UIÂÖÉÁ¥†ÔºöËøõÂ∫¶Êù° 100%ÔºåÂ∑≤ÁªèÊ∏≤ÊüìÂÆåÊàêÔºÅ";
                    KSwordKit.Core.EnhancedCoroutine.Frame.WaitTime(new System.TimeSpan(0, 0, 0, 0, 2500), () => {
                        ProgressText.text = "done!";
                    });
                });
            }
        
        },()=> progress++);
    }
}
